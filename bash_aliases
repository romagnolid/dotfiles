alias dirs="dirs -p"
alias grep='grep --color=auto'
alias less="less -Sn"
alias ls='ls --color=auto --group-directories-first'
alias la='ls -A'
alias ll='ls -laFh'
alias untar="tar xvzf"
alias upgrade="sudo apt autoremove -y; sudo apt update && sudo apt upgrade -y"
alias wcl="wc -l"
alias df="df -h"
alias du="du -h"
