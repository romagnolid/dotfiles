alias dirs="dirs -p"
alias grep='grep --color=auto'
alias la='ls -A'
alias less="less -Sn"
alias ll='ls -laFh'
alias ls='ls --color=auto --group-directories-first'
alias untar="tar xvzf"
alias upgrade="sudo apt autoremove -y; sudo apt update && sudo apt upgrade -y"
alias vi="nvim"
alias vim="nvim"
alias vimdiff="nvim -d"
alias wcl="wc -l"
alias df="df -h"
alias ds="ds -h"
